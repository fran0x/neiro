$: n("<40 4 0 9 7>*16").add("<7 _ _ 6 5 _ _ 6>*2")
  .scale("g:major").trans(-12)
  .octave(3).s("sawtooth")
  .delay(.4).pan(rand)
  .fm(.5).fmwave('white')
  .every(8, x => x.gain(0.8))
  ._pianoroll()

$: n("<7 _ _ 6 5 _ <5 3> <6 4>>*2")
  .scale("e:minor").trans(-24)
  .detune(rand)
  .octave(4).s("supersaw").gain(1.4)
  .delay(.25).pan(sine)
  ._pianoroll()

$: n("<0 1 2 3 9 8 <6 7> <5 6>>*4")
  .scale("g:major")
  .octave(6).s("piano").gain(1.2)
  .delay(.6)
  ._pianoroll()

$: s("white!4")
  .att(.4).octave(2)
  .hp(2000).gain(0.5)
  .duck("3:4:5:6").duckdepth(.4)
  ._scope()

$: s("bd:2!4")
  .duck("3:4:5:6")
  .duckdepth(.8)
  .duckattack(.16)
  ._scope()

$: s("cp:1/2")
  .fit().octave(5)
  ._scope()
